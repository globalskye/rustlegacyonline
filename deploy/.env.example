DB_PASSWORD=your_secure_password_here
DB_NAME=rustlegacy
DB_USER=rustlegacy

# /api for same-origin (nginx proxies)
REACT_APP_API_URL=/api

# http = initial (no SSL), ssl = after cert obtained
NGINX_CONFIG=http

# For Let's Encrypt notifications
CERTBOT_EMAIL=admin@rustlegacy.online
